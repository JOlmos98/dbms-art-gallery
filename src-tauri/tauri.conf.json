{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "galeria-de-arte",
  "version": "0.1.0",
  "identifier": "com.jos-dbms.art-gallery",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build"
  },
  "app": {
    "windows": [
      {
        "title": "SGBD Galer√≠a de Arte",
        "width": 1400,
        "height": 900,
        "resizable": false,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
